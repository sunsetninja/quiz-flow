    "jest-environment-jsdom-global": "^2.0.4",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^1.11.3",
