{
  "name": "cloud-frontend",
  "version": "1.3.2",
  "main": "src/index.js",
  "author": "Dmitriy Balashov <mockedcat@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "lint": "eslint src",
    "prettier": "prettier \"**/*.+(js|jsx|json)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run test && npm run prettier -- --list-different",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "test:debug": "better-npm-run test:debug",
    "test:coverage": "better-npm-run test:coverage",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 cy:open",
    "cy:run": "cypress run",
    "cy:open": "cypress open"
  },
  "betterScripts": {
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": false
      }
    },
    "test:watch": {
      "command": "jest --watch",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": false
      }
    },
    "test:coverage": {
      "command": "jest --collectCoverage",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": false
      }
    },
    "test:debug": {
      "command": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": false
      }
    },
    "dev": {
      "command": "webpack-dev-server --progress --config ./config/webpack/webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "ENV": "development",
        "API_URL": "http://api.neuroio.local/v1/",
        "IAM_URL": "http://iam.neuroio.local/v1/",
        "WS_URL": "ws://events.neuroio.local/",
        "DEBUG": true,
        "MAIN_DOMAIN": "neuroio.local"
      }
    },
    "build": {
      "command": "webpack --config ./config/webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@neuroio/api": "^4.0.2",
    "@neuroio/ui-kit": "git+https://github.com/neuroio/ui-kit.git",
    "@sentry/browser": "^5.21.0",
    "@sentry/integrations": "^5.21.0",
    "browser-cookie-lite": "^1.0.4",
    "change-case": "^4.1.1",
    "connected-react-router": "^6.8.0",
    "dayjs": "^1.8.33",
    "favicon-switcher": "^1.2.2",
    "formik": "^2.1.5",
    "history": "^4.10.1",
    "lodash-es": "^4.17.15",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-qr-svg": "^2.2.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-router-last-location": "^2.0.1",
    "react-use": "^15.3.3",
    "react-webcam": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "redux-saga-routines": "^3.2.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "uuid-validate": "0.0.3",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@cypress/webpack-preprocessor": "^5.4.3",
    "@sentry/webpack-plugin": "^1.12.0",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/dom": "^7.22.1",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "acorn-dynamic-import": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
